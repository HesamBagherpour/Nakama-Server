{
  "include": [
    "./src/**/*"
  ],
  "compilerOptions": {
    "outFile": "server/data/modules/index.js",
    "target": "es5",
    "strict": false,
    "removeComments": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "typeRoots": [
      "./node_modules"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false
  }
}
